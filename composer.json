{
    "name": "alexeevdv/yii2-gravatar-widget",
    "description": "Gravatar widget for Yii2",
    "keywords": ["yii2", "gravatar", "avatar", "widget"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/alexeevdv/yii2-gravatar-widget",
        "issues": "https://github.com/alexeevdv/yii2-gravatar-widget/issues"
    },
    "authors": [
        {
            "name": "alexeevdv",
            "homepage": "https://alexeevdv.ru/",
            "email": "mail@alexeevdv.ru"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "alexeevdv\\yii\\gravatar\\": "src"
        }
    },
    "require": {
        "yiisoft/yii2": "^2.0"
    },
    "require-dev": {
        "codeception/codeception": "^2.5",
        "yiisoft/yii2-coding-standards": "^2.0"
    }
}
